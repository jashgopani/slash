{
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.formatOnSave": true,
    "editor.autoClosingBrackets": "always",
    "linting.flake8Enabled": true,
    "linting.flake8Args": [
      "--max-line-length=120",
      "--ignore=E402,F841,F401,E302,E305,E303,E402"
    ]
  }
}
